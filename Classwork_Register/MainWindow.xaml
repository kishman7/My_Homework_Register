<Window x:Class="Classwork_Register.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Classwork_Register"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel Orientation="Vertical">
        <Label Content="Autorun"/>
        <ListBox x:Name="listBox" Height="300"/>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="1*"/>
            </Grid.ColumnDefinitions>
            <Button x:Name="btmStop" Content="Stop" Margin="5" Width="150"
                    Click="btmStop_Click"/>
            <Button Grid.Column="1" x:Name="btnRun" Content="Run"
                    Margin="5" Width="150"
                    Click="btnRun_Click"/>

        </Grid>
        

    </StackPanel>
</Window>
